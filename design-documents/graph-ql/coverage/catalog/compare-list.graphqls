type ComparableItem {
    productId: ID!
    name: String!
    sku: String!
    priceRange: ProductPriceRange!
    canonical_url: String
    images: [ProductImage]
    values: [ProductAttribute]
}

type ComparableIndicator {
    code: String!
    title: String!
}

type CompareList {
    items: [ComparableItem]
    indicators: [ComparableIndicator]
}

type Customer{
    compareList: CompareList
}

type Customer {
    compare_list: CompareList
}

type Query {
    —ÅompareList(id: ID!): CompareList
}

type Mutation {
    addItemsToCompareList(
        id: ID!
        items: [ID!]
    ): CompareList
    removeItemsFromCompareList(
        id: ID!
        items: [ID!]
    ): CompareList
    clearList(id: ID!): Boolean
    assignCompareListToCustomer(customerId: ID!, listId: ID!): Boolean
}

schema {
    query: Query,
    mutation: Mutation
}